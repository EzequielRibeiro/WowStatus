+        ExecutorService executorService = Executors.newFixedThreadPool(singletonsClass.VALUESCATEGORY.length);
+        ReentrantLock lock = new ReentrantLock();
+
+
+        for (int i = 0; i < singletonsClass.VALUESCATEGORY.length ; i++) {
+
+            int finalI = i;
+            executorService.execute(() -> {
+                        lock.lock();
+                        try {
+                            new IONRequest(getApplicationContext(), singletonsClass.VALUESCATEGORY[finalI], lock, finalI);
+                        } finally {
+                            lock.unlock();
+                        }
+
+                    }
+            );
+
+        }
+
+        new Thread(new Runnable() {
+            @Override
+            public void run() {
+
+                while (true) {
+
+                    if (!(singletonsClass.requestNewsCompleted < singletonsClass.VALUESCATEGORY.length)) {
+                        executorService.shutdownNow();
+                        break;
+                    }
+                }
+
+                Intent intent = new Intent(getApplicationContext(), MainActivity.class);
+                startActivity(intent);
+                finish();
+
+            }
+        }).start();
